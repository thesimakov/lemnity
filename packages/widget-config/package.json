{
  "name": "@lemnity/widget-config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./migrations": {
      "types": "./dist/migrations.d.ts",
      "default": "./dist/migrations.js"
    },
    "./widgets/base": {
      "types": "./dist/widgets/base.d.ts",
      "default": "./dist/widgets/base.js"
    },
    "./widgets/wheel-of-fortune": {
      "types": "./dist/widgets/WheelOfFortune/schema.d.ts",
      "default": "./dist/widgets/WheelOfFortune/schema.js"
    },
    "./widgets/action-timer": {
      "types": "./dist/widgets/ActionTimer/schema.d.ts",
      "default": "./dist/widgets/ActionTimer/schema.js"
    },
    "./widgets/fab-menu": {
      "types": "./dist/widgets/FABMenu/schema.d.ts",
      "default": "./dist/widgets/FABMenu/schema.js"
    },
    "./widgets/announcement": {
      "types": "./dist/widgets/Announcement/schema.d.ts",
      "default": "./dist/widgets/Announcement/schema.js"
    }
  },
  "dependencies": {
    "zod": "^4.1.9"
  }
}
