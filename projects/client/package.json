{
  "name": "client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build:workspace-deps": "pnpm -C ../.. --filter @lemnity/api-sdk --filter @lemnity/nest-common --filter @lemnity/widget-config --filter @lemnity/embed-script --if-present run build",
    "sync:embed-script": "pnpm -C ../.. --filter @lemnity/embed-script --if-present run sync:public",
    "postinstall": "pnpm run build:workspace-deps && pnpm run sync:embed-script",
    "start:dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format:fix": "prettier --write . --ignore-path .prettierignore",
    "format": "prettier --check . --ignore-path .prettierignore",
    "lint:fix": "eslint --fix .",
    "lint": "eslint ."
  },
  "dependencies": {
    "@heroui/breadcrumbs": "^2.2.21",
    "@heroui/button": "^2.2.25",
    "@heroui/card": "^2.2.25",
    "@heroui/checkbox": "^2.3.26",
    "@heroui/divider": "^2.2.20",
    "@heroui/dropdown": "^2.3.26",
    "@heroui/input": "^2.4.27",
    "@heroui/listbox": "^2.3.25",
    "@heroui/popover": "^2.3.29",
    "@heroui/radio": "^2.3.27",
    "@heroui/select": "^2.4.27",
    "@heroui/slider": "^2.4.24",
    "@heroui/switch": "^2.2.23",
    "@heroui/system": "^2.4.21",
    "@heroui/theme": "^2.4.21",
    "@heroui/tooltip": "^2.2.23",
    "@hookform/resolvers": "^5.2.2",
    "@lemnity/api-sdk": "workspace:*",
    "@lemnity/nest-common": "workspace:*",
    "@lemnity/widget-config": "workspace:*",
    "@tailwindcss/vite": "catalog:tailwind",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-number-format": "^5.4.4",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.6.0",
    "tailwindcss": "catalog:tailwind",
    "zod": "^4.1.9",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
